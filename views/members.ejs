      </nav>
      <!-- End Navbar -->
          <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12 ml-auto mr-auto">
              <form action="/user/create" method="post">
               <div class="row">
                      <div class="col-md-3">
                        <div class="form-group">
                          <label class="bmd-label-floating">ID</label>
                          <input type="text" class="form-control" name="id">
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-group">
                          <label class="bmd-label-floating">PASSWORD</label>
                          <input type="text" class="form-control" name="pass">
                        </div>
                      </div><div class="col-md-2">
                        <div class="form-group">
                          <label class="bmd-label-floating">NAME</label>
                          <input type="text" class="form-control" name="uname">
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group">
                          <label class="bmd-label-floating">IP</label>
                          <input type="text" class="form-control" name="ip">
                        </div>
                      </div>
                    </div>
                    <button type="submit" class="btn btn-primary pull-right" v-on:click="submit">Create</button>
                  </form>
                  <h4 style="margin-top: 40px;">Member List</h4>
                  <hr>
                  <form>
                   <div class="row">
                      <div class="col-md-3">
                        <div class="form-group">
                          <label class="bmd-label-floating">Id N.</label>
                          <input type="text" class="form-control">
                        </div>
                      </div>
                      <div class="col-md-3">
                      <button type="submit" class="btn btn-primary">Search</button>
                    </div>
                      <div class="col-md-3">
                        <div class="form-group">
                          <label class="bmd-label-floating">Name</label>
                          <input type="text" class="form-control">
                        </div>
                      </div>
                      <div class="col-md-3">
                      <button type="submit" class="btn btn-primary">Search</button>
                    </div>
                    </div>
                    <div class="row" style="margin-top: 40px;">
                      <div class="col-md-5">
                        <div class="form-group">
                          <label class="bmd-label-floating">From</label>
                          <input type="text" class="form-control">
                        </div>
                      </div>
                      <div class="col-md-5">
                        <div class="form-group">
                          <label class="bmd-label-floating">To</label>
                          <input type="text" class="form-control">
                        </div>
                      </div>
                      <div class="col-md-2">
                      <button type="submit" class="btn btn-primary">Search</button>
                    </div>
                    </div>
                  </form>
            </div>
            <table class="table table-bordered table-hover table-striped table-condensed">
               <tr>
      <th scope="col">&nbsp;</th>
      <th scope="col">Sr. No.</th>
      <th scope="col">ID</th>
      <th scope="col">NAME</th>
      <th scope="col">PASSWORD</th>
      <th scope="col">IP</th>
      <th scope="col">DATE_ENTRY</th>
      <th scope="col">API_KEY</th>
    </tr>
    <% for (i=0; i < data.length; i++) { %> 
        <tr>
          <td><a href="#">click</a></td>
          <td></td>
          <td><%-data[i].id%></td>
          <td><%-data[i].uname %></td>
          <td><%- data[i].pass %></td>
          <td><%- data[i].ip %></td>
          <td><%- data[i].date_entry %></td>
          <td><%- data[i].api_key %></td>
          
        </tr>
      <% } %>
            </table>
          </div>
        </div>
      </div>
      <% include footer.ejs %>
</body>

</html>



